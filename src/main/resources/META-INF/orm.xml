<entity-mappings xmlns="http://java.sun.com/xml/ns/persistence/orm"
		xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
		xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm orm_1_0.xsd"
		version="1.0">
<package>main</package>
<entity class="main.Reservierung" name="Reservierung" access="FIELD">
<named-query name="Reservierung.findByEmail">
	<query>select r from Reservierung r where r.Benutzer.eMail = :eMail</query>
</named-query>
<table name="Reservierung"/>
<attributes>
	<id name="ID">
		<generated-value strategy="AUTO"/>
	</id>
	<many-to-one name="benutzer" target-entity="main.Benutzer">
	</many-to-one>
	<many-to-one name="strecke" target-entity="main.Strecke">
	</many-to-one>
	<many-to-one name="zug" target-entity="main.Zug">

	</many-to-one>
	<transient name="zahlung"/>
</attributes>
</entity>
